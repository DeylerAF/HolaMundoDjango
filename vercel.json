{
    "builds": [
        {
            "src": "HolaMundoDjango/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "HolaMundoDjango/wsgi.py"
        }
    ]
}